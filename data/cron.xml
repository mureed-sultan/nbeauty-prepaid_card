<odoo>
    <data noupdate="1">
        <record id="ir_cron_auto_expire_prepaid_cards" model="ir.cron">
            <field name="name">Auto Expire Prepaid Cards</field>
            <field name="model_id" ref="model_nbeauty_prepaid_card"/>
            <field name="state">code</field>
            <field name="code">model.auto_expire_cards()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>
